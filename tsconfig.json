{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": "./",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@components/*": ["./components/*"],
      "@pages/*": ["./pages/*"],
      "@public/*": ["./public/*"],
      
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/auth/auth.ts", "components/auth/prisma.ts", "components/auth/types.ts", "components/auth/user.ts", "prima.js", "utils/userStuff.js", "utils/router.ts.exit"],
  "exclude": ["node_modules"]
}
